{
    "dashboard": {
        "id": null,
        "title": "ORFEAS Enterprise Agent System",
        "tags": [
            "orfeas",
            "agents",
            "enterprise"
        ],
        "style": "dark",
        "timezone": "browser",
        "editable": true,
        "graphTooltip": 0,
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "timepicker": {
            "refresh_intervals": [
                "5s",
                "10s",
                "30s",
                "1m",
                "5m",
                "15m",
                "30m",
                "1h",
                "2h",
                "1d"
            ],
            "time_options": [
                "5m",
                "15m",
                "1h",
                "6h",
                "12h",
                "24h",
                "2d",
                "7d",
                "30d"
            ]
        },
        "refresh": "30s",
        "panels": [
            {
                "id": 1,
                "title": "Agent System Overview",
                "type": "stat",
                "targets": [
                    {
                        "expr": "up{job=\"orfeas-agents\"}",
                        "legendFormat": "Agent System Status"
                    },
                    {
                        "expr": "orfeas_agent_count",
                        "legendFormat": "Active Agents"
                    },
                    {
                        "expr": "rate(orfeas_agent_requests_total[5m])",
                        "legendFormat": "Requests/sec"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "yellow",
                                    "value": 0.8
                                },
                                {
                                    "color": "green",
                                    "value": 1
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Agent Performance Metrics",
                "type": "graph",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(orfeas_agent_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "95th Percentile Response Time"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(orfeas_agent_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "Median Response Time"
                    },
                    {
                        "expr": "rate(orfeas_agent_errors_total[5m])",
                        "legendFormat": "Error Rate"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Time (seconds)",
                        "min": 0
                    },
                    {
                        "label": "Errors/sec",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 3,
                "title": "Agent Communication System",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(orfeas_agent_communication_messages_total[5m])",
                        "legendFormat": "Messages/sec"
                    },
                    {
                        "expr": "orfeas_agent_communication_queue_size",
                        "legendFormat": "Queue Size"
                    },
                    {
                        "expr": "rate(orfeas_agent_communication_failures_total[5m])",
                        "legendFormat": "Communication Failures/sec"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Count",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            },
            {
                "id": 4,
                "title": "Agent Types Distribution",
                "type": "piechart",
                "targets": [
                    {
                        "expr": "orfeas_agent_count by (agent_type)",
                        "legendFormat": "{{agent_type}}"
                    }
                ],
                "options": {
                    "legend": {
                        "displayMode": "table",
                        "placement": "right"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 0,
                    "y": 16
                }
            },
            {
                "id": 5,
                "title": "Quality Assessment Metrics",
                "type": "graph",
                "targets": [
                    {
                        "expr": "orfeas_generation_quality_score",
                        "legendFormat": "Average Quality Score"
                    },
                    {
                        "expr": "rate(orfeas_generation_total[5m])",
                        "legendFormat": "Generations/sec"
                    },
                    {
                        "expr": "rate(orfeas_generation_failures_total[5m])",
                        "legendFormat": "Failures/sec"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Quality Score / Rate",
                        "min": 0,
                        "max": 1
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 8,
                    "y": 16
                }
            },
            {
                "id": 6,
                "title": "System Resources",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(process_cpu_seconds_total{job=\"orfeas-backend\"}[5m]) * 100",
                        "legendFormat": "CPU Usage %"
                    },
                    {
                        "expr": "(process_resident_memory_bytes{job=\"orfeas-backend\"} / (1024^3))",
                        "legendFormat": "Memory Usage (GB)"
                    },
                    {
                        "expr": "orfeas_gpu_utilization_percent",
                        "legendFormat": "GPU Utilization %"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Percentage / GB",
                        "min": 0,
                        "max": 100
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 16,
                    "y": 16
                }
            },
            {
                "id": 7,
                "title": "Agent Load Balancing",
                "type": "graph",
                "targets": [
                    {
                        "expr": "orfeas_agent_load_balancer_requests_total by (agent_id)",
                        "legendFormat": "Requests to {{agent_id}}"
                    },
                    {
                        "expr": "orfeas_agent_health_score by (agent_id)",
                        "legendFormat": "Health Score {{agent_id}}"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Requests / Health Score",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 24
                }
            },
            {
                "id": 8,
                "title": "Redis Communication Metrics",
                "type": "graph",
                "targets": [
                    {
                        "expr": "redis_connected_clients",
                        "legendFormat": "Connected Clients"
                    },
                    {
                        "expr": "rate(redis_total_commands_processed[5m])",
                        "legendFormat": "Commands/sec"
                    },
                    {
                        "expr": "redis_memory_used_bytes / (1024^2)",
                        "legendFormat": "Memory Usage (MB)"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Count / MB",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 24
                }
            },
            {
                "id": 9,
                "title": "Agent Workflow Orchestration",
                "type": "table",
                "targets": [
                    {
                        "expr": "orfeas_agent_workflow_duration_seconds by (workflow_type)",
                        "legendFormat": "{{workflow_type}}"
                    }
                ],
                "transform": {
                    "id": "organize",
                    "options": {
                        "excludeByName": {},
                        "indexByName": {},
                        "renameByName": {
                            "Value": "Duration (seconds)",
                            "workflow_type": "Workflow Type"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 32
                }
            },
            {
                "id": 10,
                "title": "Error Analysis",
                "type": "logs",
                "targets": [
                    {
                        "expr": "{job=\"orfeas-backend\"} |= \"ERROR\" or |= \"CRITICAL\"",
                        "legendFormat": "Error Logs"
                    }
                ],
                "options": {
                    "showTime": true,
                    "showLabels": true,
                    "sortOrder": "Descending"
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 32
                }
            }
        ],
        "templating": {
            "list": [
                {
                    "name": "instance",
                    "type": "query",
                    "query": "label_values(up{job=\"orfeas-backend\"}, instance)",
                    "refresh": 1,
                    "includeAll": true,
                    "multi": true
                },
                {
                    "name": "agent_type",
                    "type": "query",
                    "query": "label_values(orfeas_agent_count, agent_type)",
                    "refresh": 1,
                    "includeAll": true,
                    "multi": true
                }
            ]
        },
        "annotations": {
            "list": [
                {
                    "name": "Deployments",
                    "datasource": "prometheus",
                    "expr": "changes(up{job=\"orfeas-backend\"}[1m]) > 0",
                    "titleFormat": "Deployment",
                    "textFormat": "Service restarted"
                }
            ]
        }
    }
}
